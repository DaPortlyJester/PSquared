<?xml version="1.0" encoding="utf-8" standalone="no"?><web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.5" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	<servlet>
		<servlet-name>PSquared</servlet-name>
		<servlet-class>com.umich.umd.psquared.PSquaredServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>PSquared</servlet-name>
		<url-pattern>/psquared</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>jsppages/psquared.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>sign</servlet-name>
		<servlet-class>com.umich.umd.psquared.SignGuestbookServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>sign</servlet-name>
		<url-pattern>/sign</url-pattern>
	</servlet-mapping>
		<servlet>
	    <servlet-name>Tutoring</servlet-name>
	    <servlet-class>com.umich.umd.psquared.TutoringServlet</servlet-class>
	</servlet>
	<servlet-mapping>
	    <servlet-name>Tutoring</servlet-name>
	    <url-pattern>/tutoring</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>SystemServiceServlet</servlet-name>
		<servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
		<init-param>
			<param-name>services</param-name>
			<param-value/>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>SystemServiceServlet</servlet-name>
		<url-pattern>/_ah/spi/*</url-pattern>
	</servlet-mapping>
</web-app>